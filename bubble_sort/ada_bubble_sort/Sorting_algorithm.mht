/* Edit window toolbar */
#toolbar {
    height: 22px;
    margin-bottom: 6px;
}

/* Margins for <ol> and <ul> */
#content ol, #content ul,
#mw_content ol, #mw_content ul {
  margin-bottom: 0.5em;
}

/* Make the list of references in [[Template:Reflist]] smaller */
.references-small {=
=20
    font-size: 90%;
